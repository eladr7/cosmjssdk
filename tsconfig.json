        "sourceMap": false,
        "noImplicitAny": true,
        "esModuleInterop": true,
        "module": "commonjs",
        "target": "es2017",
        "allowJs": true,
        "moduleResolution": "node"
    },
    "include": [
        "./src/**/*"
